<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(map_label) -%>">
  <span class='title'>
    <%= map_label.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), map_label_url(map_label),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_map_label_path(map_label),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_map_label_path(map_label),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => map_label.title),
        :method => :delete %>
  </span>
</li>
